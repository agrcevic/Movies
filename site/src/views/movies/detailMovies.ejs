<section>
    <% if(detailMovie){ %>
        <% for(let x=1; x < 10; x++) { %>
            <h2><%= x %>:<%= detailMovie.titulo %></h2>
            <p><%= detailMovie.tipo%></p>
            <p><%= detailMovie.actor%></p>
        <% } %>
    <% }else{ %>
        <h2>NO HAY NADA PARA MOSTRAR</h2>
    <% } %>
</section>
<!-- CONTENIDO PRINCIPAL FIN-->
